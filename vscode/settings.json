{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "editor.lineHeight": 24,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [80],
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Solarized Dark]": {
      "textMateRules": [
        {
          "scope": [
            "markup.list.unnumbered.markdown markup.list.unnumbered.markdown",
            "markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown"
          ],
          "settings": {
            "foreground": "#6c71c4"
          }
        },
        {
          "scope": [
            "markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown",
            "markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown"
          ],
          "settings": {
            "foreground": "#dc322f"
          }
        },
        {
          "scope": [
            "markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown markup.list.unnumbered.markdown"
          ],
          "settings": {
            "foreground": "#b58900"
          }
        }
      ]
    }
  },
  "explorer.compactFolders": false,
  "git.enableSmartCommit": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "ruby.rubocop.executePath": "/home/ysuss/.rbenv/shims/",
  "ruby.rubocop.onSave": false,
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "ruby.lint": {
    "rubocop": {
      "useBundler": true
    },
    "reek": {
      "useBundler": true
    }
  },
  "ruby.format": "rubocop",
  "sqltools.format": {
    "language": "sql",
    "reservedWordCase": "upper",
    "linesBetweenQueries": 2
  },
  "sqltools.formatLanguages": ["sql"],
  "telemetry.enableTelemetry": false,
  "workbench.editor.enablePreview": false,
  "workbench.colorTheme": "Solarized Dark",
  "workbench.panel.defaultLocation": "right",
  "workbench.startupEditor": "newUntitledFile"
}
